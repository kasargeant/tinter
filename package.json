{
  "name": "tinter",
  "version": "0.0.3",
  "description": "A lean and fast console string colorizing module - supports CSS naming and up to 16M+ colors.",
  "keywords": [
    "javascript"
  ],
  "author": "Kyle Alexis Sargeant",
  "email": "kasargeant@gmail.com",
  "license": "LGPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/kasargeant/tinter.git"
  },
  "main": "dist/index.js",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "engines": {
    "node": "~6.10.0",
    "npm": "~3.10.10"
  },
  "scripts": {
    "build": "warhorse build",
    "clean": "warhorse clean",
    "create": "warhorse create",
    "distribute": "warhorse distribute",
    "document": "warhorse document",
    "init": "warhorse init",
    "lint": "warhorse lint",
    "pack": "warhorse pack",
    "precompile": "warhorse precompile",
    "test": "warhorse test",
    "jest": "jest",
    "jest-update": "jest --updateSnapshot"
  },
  "devDependencies": {
    "minami": "^1.2.3",
    "jest": "^20.0.4",
    "jest-html-reporter": "^0.1.0"
  },
  "jest-html-reporter": {
    "pageTitle": "Test Results",
    "outputPath": "docs/tests/index.html",
    "includeFailureMsg": false
  }
}
