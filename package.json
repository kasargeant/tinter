{
  "name": "tinter",
  "version": "0.4.5",
  "description": "A lean and fast console string colorizing module - supports CSS naming and up to 16M+ colors.",
  "keywords": [
    "javascript",
    "color",
    "colour",
    "truecolor",
    "console",
    "terminal",
    "ANSI",
    "CSS"
  ],
  "author": "Kyle Alexis Sargeant",
  "email": "kasargeant@gmail.com",
  "license": "LGPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/kasargeant/tinter.git"
  },
  "main": "src/index.js",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "engines": {
    "node": "~6.10.0",
    "npm": "~3.10.10"
  },
  "scripts": {
    "test": "jest",
    "test-cover": "jest --coverage",
    "test-update": "jest --updateSnapshot"
  },
  "devDependencies": {
    "coveralls": "^2.13.1",
    "jest": "^20.0.4",
    "jest-html-reporter": "^0.1.0",
    "minami": "^1.2.3"
  },
  "jest-html-reporter": {
    "pageTitle": "Test Results",
    "outputPath": "docs/tests/index.html",
    "includeFailureMsg": false
  }
}
